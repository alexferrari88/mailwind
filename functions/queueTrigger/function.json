{
  "bindings": [
    {
      "name": "queueItem",
      "type": "queueTrigger",
      "direction": "in",
      "queueName": "rendered-html",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "renderedBlob",
      "type": "blob",
      "path": "html/{issueId}.html",
      "direction": "out",
      "connection": "AzureWebJobsStorage"
    },
    {
     "name": "emailBlob",
     "type": "blob",
     "path": "emails/latest.html",
     "direction": "out",
     "connection": "AzureWebJobsStorage"
    },
    {
      "name": "outQueue",
      "type": "queue",
      "queueName": "ready-to-send",
      "direction": "out",
      "connection": "AzureWebJobsStorage"
    }
  ],
  "scriptFile": "../dist/queueTrigger/index.js"
}